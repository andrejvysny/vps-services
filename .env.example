# =============================================================================
# Environment Variables Template for services-stack
# =============================================================================
# Copy this to .env and fill in real values
# Generate secrets with: openssl rand -base64 32

# Traefik ACME (Let's Encrypt)
ACME_EMAIL=your-email@example.com
ACME_CA_SERVER=https://acme-v02.api.letsencrypt.org/directory
TRAEFIK_LOG_LEVEL=INFO

# PostgreSQL superuser (admin access)
POSTGRES_SUPERUSER_PASSWORD=CHANGE_ME

# n8n database credentials
N8N_DB_PASSWORD=CHANGE_ME
N8N_ENCRYPTION_KEY=CHANGE_ME

# Postiz database credentials
POSTIZ_DB_PASSWORD=CHANGE_ME
POSTIZ_JWT_SECRET=CHANGE_ME
